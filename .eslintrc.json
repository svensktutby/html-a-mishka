{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "max-len": [2, 120, 8],
    "no-console":"off"
  }
}
